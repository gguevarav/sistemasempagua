-bind:key=\"index\">\n    <v-col>\n      <v-card\n      :loading=\"loading\"\n      class=\"mx-auto my-12\"\n      max-width=\"374\"\n    >\n      <template slot=\"progress\">\n        <v-progress-linear\n          color=\"deep-purple\"\n          height=\"10\"\n          indeterminate\n        ></v-progress-linear>\n      </template>\n\n      <v-img\n        height=\"250\"\n        :src=\"LogoUsar\"\n      ></v-img>\n\n      <v-card-title> {{ item.NombreSistema }} </v-card-title>\n\n      <v-card-text>\n\n        <div class=\"my-4 subtitle-1\">\n          <a :href=\"item.DireccionSistema\">{{ item.DireccionSistema }}</a>\n        </div>\n\n        <div> {{ item.DescripcionSistema }}</div>\n      </v-card-text>\n\n      <v-divider class=\"mx-4\"></v-divider>\n\n      <v-card-actions>\n        <v-btn\n          color=\"deep-purple lighten-2\"\n          text\n          @click=\"reserve\"\n        >\n          Reserve\n        </v-btn>\n      </v-card-actions>\n    </v-card>\n    </v-col>\n  </v-row>\n  </v-container>\n</template>\n\n<script>\nimport axios from \"axios\"\n\nexport default {\n  name: 'Home',\n  data: () => ({\n    listadoSistemas: [],\n  }),\n  created(){\n    this.inicializar();\n  },\n  methods: {\n    inicializar(){\n      new Promise((resolve, reject) => {\n        axios.get(\"/sistema\")\n            .then(response => {\n              if (response.data.total != 0) {\n                this.listadoSistemas = response.data.detalle\n                console.log(response.data.detalle);\n              }\n            })\n            .catch(function (error) {\n              console.log(error);\n            })\n      });\n    },\n\n    LogoUsar: item => '@assets/logossistemas/' + item.LogoSistema\n  },\n}\n</script>\n"],"sourceRoot":"src/views"}]}